{
    "name": "Ad blocker",
    "version": "1.0.0",
    "manifest_version" : 3,
    "permissions" : [
        "declarativeNetRequest", "tabs","storage", "activeTab"
    ],
    "declarative_net_request" : { 
        "rule_resources" : [
            {
                "id" : "ruleset_1",
                "enabled" : true,
                "path": "rules.json"
            }
        ]
    },
    "action" : {
        "default_icon" : "adblock.png",
        "default_popup" : "popup.html"
    }
}