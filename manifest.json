{
    "name": "Ad blocker",
    "version": "1.0.0",
    "manifest_version" : 2,
    "permissions" : [
        "tabs","storage", "activeTab","webRequest", "webRequestBlocking", "<all_urls>"
    ],
    "background": {
        "scripts": ["rules.js"]
    },
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
      },
   
    "action" : {
        "default_icon" : "adblock.png",
        "default_popup" : "popup.html"
    }
}